<% if obj.errors.any? %>
  <div class = "row">
    <div class = "alert alert-danger col-md-8 col-md-offset-2">
      <h3 class = "center">Please correct the following errors:</h3>
       <ul class = "ul_msg">
          <% obj.errors.full_messages.each do |msg| %>
            <li><%= msg %></li>
          <% end %>
          </ul>
      </div>
  <div>
<% end %>